<% if (typeof playlists != 'undefined') { %>
	<div>
		<h3><a href="/playlist/new">Create New Playlist</a></h3>
	</div>
	<% playlists.forEach((item, index) => { %>
		<%- include('./playlistthumbnail.ejs', {playlist: item}) %>
	<% }); %>
<% } else { %>
	<h3><a href="/playlist/new">Create New Playlist</a></h3>
<% } %>
